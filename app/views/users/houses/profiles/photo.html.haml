= notice_message
%h1.inline= t ".title"
%span.ajax-text
%hr
= simple_form_for [@user, @house], html: { multipart: true } do |f|
  = f.error_notification

  .form-inputs
    = t "images"
    = render 'users/houses/partials/house_images'
    = file_field_tag "image[]", multiple: true
  
  = f.input :photo_help, as: :boolean
    
  = f.button :button do
    = t ".upload_photo"